    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "@algolia/client-search": "^4.9.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.2.0",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/react": "^18.0.25",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "typescript": "^4.9.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
